{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "./.auto-imports/auto-imports.d.ts",
    "./.auto-imports/components.d.ts",
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue"
  ],
  "exclude": ["src/**/__tests__/*"],
  "compilerOptions": {
    "composite": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    }
  }
}
